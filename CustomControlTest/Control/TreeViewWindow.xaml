<Window x:Class="CustomControlTest.Control.TreeViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomControlTest.Control"
        xmlns:vm="clr-namespace:CustomControlTest.ViewModel"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance vm:TreeViewWindowViewModel}"
        Title="树状图控件" Height="450" Width="700"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize"
        >
    <Grid>
        <ContentControl Content="{Binding TreeViewControl}"></ContentControl>
    </Grid>
</Window>
